console.log('<%= "oui" %>');
$(document).ready(function(){
	let checkStatus = null;
	let nowStatus = null;
	let gameBoard = "#game-board"
	setInterval( function() {
		checkStatus = <%= party.check_status %>;
		if (checkStatus != nowStatus) {
			nowStatus = checkStatus;
			$(gameBoard).empty();
			switch(nowStatus) {
				case 0 :
				$(gameBoard).html(<%= j render 'init' %>);
				break;
				case 1 :
				$(gameBoard).html(<%= j render 'party' %>);
				break;
				case 2 :
				$(gameBoard).html(<%= j render 'ended' %>);
				break;
			}
		}
	}, 500);
});