<%= image_tag user.avatar, alt: "#{user.username}'s  avatar" %>
<% end %>
<ul id="user<%= user.id %>_info">
  <li><%= user.username %></li>
  <li><%= user.first_name %></li>
  <li><%= user.last_name %></li>
  <li><%= user.email %></li>
